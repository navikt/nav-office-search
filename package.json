{
    "name": "nav-office-search",
    "version": "1.2.6",
    "private": true,
    "type": "module",
    "scripts": {
        "typeCheck": "tsc --noemit",
        "build": "tsc && pnpm build:client && pnpm build:ssr && pnpm build:server",
        "build:client": "vite build --outDir server/frontendDist/client",
        "build:ssr": "vite build --outDir server/frontendDist/ssr --ssr src/main-server.tsx",
        "build:server": "pnpm --filter nav-office-search-server build",
        "start": "pnpm --filter nav-office-search-server start",
        "lint": "eslint .",
        "decorator": "docker compose up",
        "dev": "pnpm --filter nav-office-search-server dev",
        "prod-local": "cp .env.local-prod .env && pnpm --filter nav-office-search-server prod-local",
        "test": "jest",
        "prepare": "husky || true"
    },
    "dependenciesComments": {
        "fetch-mock": "Holdes pÃ¥ versjon 11.1.15. Nyeste versjon 12.6.0"
    },
    "dependencies": {
        "@navikt/ds-css": "7.38.0",
        "@navikt/ds-icons": "3.4.3",
        "@navikt/ds-react": "7.38.0",
        "@navikt/nav-dekoratoren-moduler": "3.6.2",
        "fetch-mock": "11.1.5",
        "lodash.debounce": "4.0.8",
        "preact": "10.28.3",
        "preact-render-to-string": "6.6.5",
        "react": "18.3.1",
        "react-dom": "18.3.1"
    },
    "devDependencies": {
        "@babel/preset-react": "7.28.5",
        "@eslint/js": "9.39.2",
        "@preact/preset-vite": "2.10.3",
        "@testing-library/jest-dom": "6.9.1",
        "@testing-library/react": "16.3.2",
        "@types/jest": "30.0.0",
        "@types/lodash.debounce": "4.0.9",
        "@types/react": "18.3.28",
        "@types/react-dom": "18.3.7",
        "eslint": "9.39.2",
        "eslint-plugin-react": "7.37.5",
        "husky": "9.1.7",
        "identity-obj-proxy": "3.0.0",
        "jest": "30.2.0",
        "jest-environment-jsdom": "30.2.0",
        "jest-fetch-mock": "3.0.3",
        "lint-staged": "16.2.7",
        "prettier": "3.8.1",
        "ts-jest": "29.4.6",
        "typescript": "5.9.3",
        "typescript-eslint": "8.56.0",
        "vite": "7.3.1"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx,scss,css}": "prettier --write"
    }
}
