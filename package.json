{
    "name": "nav-office-search",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "typeCheck": "tsc --noemit",
        "deploy": "npx run-func trigger-workflow.js triggerWorkflow",
        "deploy-dev": "npm run deploy -- deploy.dev.yml",
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview"
    },
    "dependencies": {
        "@navikt/ds-css": "2.1.5",
        "@navikt/ds-react": "2.1.5",
        "@navikt/nav-dekoratoren-moduler": "1.9.0",
        "@preact/compat": "17.1.2",
        "amplitude-js": "8.21.3",
        "classnames": "2.3.2",
        "lodash.debounce": "4.0.8",
        "node-cache": "5.1.2",
        "node-fetch": "3.3.0",
        "node-schedule": "2.1.1"
    },
    "devDependencies": {
        "@octokit/core": "4.2.0",
        "@types/lodash.debounce": "4.0.7",
        "@types/node": "18.11.18",
        "@types/node-fetch": "2.6.2",
        "@types/node-schedule": "2.1.0",
        "@types/react": "18.0.27",
        "@types/react-dom": "18.0.10",
        "@typescript-eslint/eslint-plugin": "5.50.0",
        "@typescript-eslint/parser": "5.50.0",
        "@vitejs/plugin-react-swc": "3.0.0",
        "@preact/preset-vite": "2.5.0",
        "eslint": "8.33.0",
        "eslint-plugin-react": "7.32.2",
        "fetch-mock": "9.11.0",
        "husky": "8.0.3",
        "prettier": "2.8.3",
        "typescript": "4.9.5",
        "vite": "4.1.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "npm run typeCheck && npm run lint"
        }
    },
    "lint-staged": {
        "*.{js,jsx,json,md,ts,tsx,less, css}": "prettier --write"
    }
}
