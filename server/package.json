{
    "name": "nav-office-search-server",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "rm -fr dist && tsc",
        "start": "node dist/server/src/server.js",
        "start-local": "npm run build && concurrently \"tsc -w\" \"nodemon -w dist/server dist/server/src/server.js\"",
        "prod-local": "cp .env.local-prod .env && npm run start-local",
        "dev": "cp .env.development .env && npm run start-local"
    },
    "dependenciesComments": {
        "@types/express": "Holdes uendret 4.17.21 --> 5.0.1"
    },
    "dependencies": {
        "compression": "1.8.0",
        "dotenv": "16.4.7",
        "express": "4.21.2",
        "lodash.debounce": "4.0.8",
        "lru-cache": "11.1.0",
        "node-cache": "5.1.2",
        "node-fetch": "3.3.2",
        "node-schedule": "2.1.1",
        "vite": "6.2.4"
    },
    "devDependencies": {
        "@types/compression": "1.7.5",
        "@types/express": "4.17.21",
        "@types/lru-cache": "7.10.10",
        "@types/node": "22.13.14",
        "@types/node-schedule": "2.1.7",
        "concurrently": "9.1.2",
        "nodemon": "3.1.9"
    }
}
