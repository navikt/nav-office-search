{
    "name": "nav-office-search-server",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "rm -fr dist && tsc",
        "start": "node dist/server/src/server.js",
        "start-local": "pnpm build && concurrently \"tsc -w\" \"nodemon -w dist/server dist/server/src/server.js\"",
        "prod-local": "cp .env.local-prod .env && pnpm start-local",
        "dev": "cp .env.development .env && pnpm start-local"
    },
    "dependenciesComments": {
        "@types/node": "Holdes p√• versjon 24.10.1 ellers brekker det. Nyeste versjon 25.0.6"
    },
    "dependencies": {
        "compression": "1.8.1",
        "dotenv": "17.2.4",
        "express": "5.2.1",
        "lru-cache": "11.2.5",
        "node-cache": "5.1.2",
        "node-schedule": "2.1.1",
        "vite": "7.3.1"
    },
    "devDependencies": {
        "@types/compression": "1.8.1",
        "@types/express": "5.0.6",
        "@types/node": "24.10.1",
        "@types/node-schedule": "2.1.8",
        "concurrently": "9.2.1",
        "nodemon": "3.1.11"
    }
}
