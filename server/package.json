{
  "name": "nav-office-search-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rm -fr dist && tsc",
    "start": "node dist/server/src/server.js",
    "start-local": "npm run build && concurrently \"tsc -w\" \"nodemon -w dist/server dist/server/src/server.js\"",
    "prod-local": "cp .env.local .env && npm run start-local",
    "dev": "cp .env.development .env && npm run start-local"
  },
  "dependencies": {
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "node-cache": "5.1.2",
    "node-fetch": "2.6.9",
    "node-schedule": "2.1.1",
    "lodash.debounce": "4.0.8",
    "vite": "4.1.0"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/node": "18.11.18",
    "@types/node-schedule": "2.1.0",
    "concurrently": "7.6.0",
    "nodemon": "2.0.20"
  }
}
