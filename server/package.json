    "lodash.debounce": "4.0.8",
    "react": "npm:@preact/compat",
    "react-dom": "npm:@preact/compat"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/node": "18.11.18",
    "@types/node-fetch": "2.6.2",
    "@types/node-schedule": "2.1.0",
    "concurrently": "7.6.0",
    "nodemon": "2.0.20",
    "vite": "4.1.0"
