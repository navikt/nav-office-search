{
    "name": "nav-office-search-server",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "rm -fr dist && tsc",
        "start": "node dist/server/src/server.js",
        "start-local": "npm run build && concurrently \"tsc -w\" \"nodemon -w dist/server dist/server/src/server.js\"",
        "prod-local": "cp .env.local-prod .env && npm run start-local",
        "dev": "cp .env.development .env && npm run start-local"
    },
    "dependenciesComments": {
        "@types/express": "Holdes på versjon 4.17.21. Nyeste versjon 5.0.5",
        "express": "Holdes på versjon 4.21.2. Nyeste versjon 5.1.0",
        "vite": "Holdes på versjon 6.3.5. Nyeste versjon 7.2.2"
    },
    "dependencies": {
        "compression": "1.8.1",
        "dotenv": "17.2.3",
        "express": "4.21.2",
        "lodash.debounce": "4.0.8",
        "lru-cache": "11.2.2",
        "node-cache": "5.1.2",
        "node-schedule": "2.1.1",
        "vite": "7.2.4"
    },
    "devDependencies": {
        "@types/compression": "1.8.1",
        "@types/express": "4.17.21",
        "@types/node": "24.10.1",
        "@types/node-schedule": "2.1.8",
        "concurrently": "9.2.1",
        "nodemon": "3.1.11"
    }
}
